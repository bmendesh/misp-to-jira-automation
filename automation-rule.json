{
  "name": "Create SOC Issue from MISP IoC",
  "trigger": {
    "type": "incomingWebhook"
  },
  "actions": [
    {
      "type": "createIssue",
      "fields": {
        "project": "SOC",
        "issuetype": "Task",
        "summary": "Verify IoC from CTI: {{webhookData.ioc}}",
        "description": "Type: {{webhookData.type}}\nIoC: {{webhookData.ioc}}\nThreat Level: {{webhookData.threat_level}}\nDetails: {{webhookData.description}}",
        "labels": ["from-misp", "cti"]
      }
    }
  ]
}
